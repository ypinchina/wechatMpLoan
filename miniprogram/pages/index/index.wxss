swiper{
  width: 100%;
  height: 340rpx;
}
swiper image {
  width: 100%;
  display: block;
  height: 100%;
}
.homeItem {
  overflow: hidden;
  padding: 20rpx;
  border-bottom: 1px solid #f1f1f1;
}
.left {
  float: left;
  margin-right: 20rpx;
}

.pro-logo {
  width: 190rpx;
  height: 190rpx;
  border-radius: 8px;
}
.title {
  color: #212121;
  font-size: 16px;
  font-weight: bold;
}
.content {
  color: #9a9a9a;
  font-size: 11px;
}
.btnBox{
  overflow: hidden;
}
.detailBtn, .contact {
  display: inline-block;
}
.detailBtn{
  margin-right: 20rpx;
  height: auto;
}
.detailBtn image{
  width: 170rpx;
  height: 52rpx;
  float: left;
}
.contact{
  float: left;
  width: 224rpx;
  height: 52rpx;
}
.btnBox button{
  padding: 0;
  line-height: 1;
  display: inline-block;
  width: auto;
  min-height: auto;
  height: auto;
  margin-top: 2px;
}